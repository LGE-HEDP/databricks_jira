     "title": "음성 검색 app 사용 횟수 / UD"
    }
   },
   "outputs": [],
   "source": [
    "%sql\n",
    "with mart_voice as (\n",
    "  SELECT  x_device_sales_model, mac_addr \n",
    "  FROM    kic_data_ods.tlamp.normal_log_webos25\n",
    "  WHERE   1=1\n",
    "      AND X_device_country = 'KR'\n",
    "      AND (context_name = 'voice' AND message_id = 'NL_ACTIVATE_VOICE')\n",
    "      AND X_device_product = 'webOSTV 25'\n",
    "      AND date_ym >= '2025-10'\n",
    "      AND date_ym <= '2025-11'  -- `25.10.01 ~ `25.11\n",
    "), fact_voice_webos25 as (\n",
    "  -- webos25 음성 검색 app 사용 횟수 / UD\n",
    "  SELECT  'webos25 all' as target, count(1) as access, count(distinct mac_addr) as ud\n",
    "  FROM    mart_voice\n",
    "), fact_voice_senior as (\n",
    "  -- 시니어TV의 음성 검색 app 사용 횟수 / UD\n",
    "  SELECT  'webos25 시니어TV' as target, count(1) as access, count(distinct mac_addr) as ud\n",
    "  FROM    mart_voice\n",
    "  WHERE   1=1\n",
    "      AND (x_device_sales_model LIKE '65QNED85AKS%' OR x_device_sales_model LIKE '75QNED85AKS%') \n",
    "), mart_tv_on as (\n",
    "    SELECT x_device_sales_model, mac_addr -- \n",
    "    FROM kic_data_ods.tlamp.normal_log_webos25\n",
    "    WHERE 1=1\n",
    "        AND X_device_country = 'KR'\n",
    "        AND (context_name = 'tvpowerd' AND message_id = 'NL_POWER_STATE')\n",
    "        AND X_device_product = 'webOSTV 25'\n",
    "        AND date_ym >= '2025-10'\n",
    "        AND date_ym <= '2025-11'  -- `25.10.01 ~ `25.11\n",
    "), fact_tv_on_webos25 as (\n",
    "  -- webos25 tv on user\n",
    "    select count(distinct mac_addr) as tv_on_user\n",
    "    from   mart_tv_on\n",
    "), fact_tv_on_senior as (\n",
    "  -- 시니어TV의 tv on user\n",
    "    select count(distinct mac_addr) as tv_on_user\n",
    "    from   mart_tv_on\n",
    "    where  1=1 \n",
    "      AND  (x_device_sales_model LIKE '65QNED85AKS%' OR x_device_sales_model LIKE '75QNED85AKS%')\n",
    ")\n",
    "\n",
    "\n",
    "(select * from fact_voice_webos25, fact_tv_on_webos25)\n",
    "union all \n",
    "(select * from fact_voice_senior, fact_tv_on_senior)"
   ]
  },
