   "source": [
    "df = spark.sql('''\n",
    "    SELECT \n",
    "    nl.mac_addr AS MAC,\n",
    "    nl.log_create_time AS log_time,\n",
    "    nl.normal_log:action_type AS action_type,\n",
    "    nl.normal_log:user_utterance AS user_utterance,\n",
    "    NULL AS schedule_program_title,\n",
    "    NULL AS channel_name,\n",
    "    NULL AS channel_code,\n",
    "    NULL AS genre_name,\n",
    "    NULL AS lg_genre_code\n",
    "\n",
    "    FROM kic_data_ods.tlamp.normal_log_webos23 nl\n",
    "\n",
    "    WHERE 1=1\n",
    "    AND date_format(nl.log_create_time, 'yyyy-MM-dd') BETWEEN '2024-01-01' AND '2024-06-30'\n",
    "    AND nl.X_Device_Country = 'KR'\n",
    "    AND nl.context_name IN ('voice') -- (partition column)\n",
    "    AND nl.message_id IN ('NL_RESULT_DATA')-- (partition column)\n",
    "\n",
    "    UNION ALL\n",
    "\n",
    "    SELECT \n",
    "    ucm.mac AS MAC,\n",
    "    ucm.log_time AS log_time,\n",
    "    NULL AS action_type,\n",
    "    NULL AS user_utterance,\n",
    "    ucm.schedule_program_title,\n",
    "    ucm.channel_name,\n",
    "    ucm.channel_code,  \n",
    "    ucm.genre_name,\n",
    "    ucm.lg_genre_code\n",
    "\n",
    "    FROM sandbox.z_yookyoung_choi.ucm_log_webos23_kr ucm\n",
    "\n",
    "    WHERE 1=1\n",
    "    AND date_format(ucm.log_time, 'yyyy-MM-dd') BETWEEN '2024-01-01' AND '2024-06-30'\n",
    "\n",
    "    ORDER BY MAC ASC, log_time ASC;  -- 또는 DESC\n",
    "''')"
   ]
