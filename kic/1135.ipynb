    "%sql\n",
    "select *\n",
    "from kic_data_ods.tlamp.activation_date\n",
    "where 1=1\n",
    "  and Cntry_CODE = \"KR\"\n",
    "  and Platform_code like \"%W%\"\n",
    "  and try_cast(substr(Platform_code, 2, 2) as int) >= 21\n",
    "  and he_etl_dt < cast('2025-11-01' as timestamp)\n"
