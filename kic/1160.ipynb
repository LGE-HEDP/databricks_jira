   "source": [
    "%sql\n",
    "with raw_data as (\n",
    "  SELECT \n",
    "      X_device_platform as `platform_code`,  -- 필수 열\n",
    "      X_device_product as `platform_version`,-- 필수 열\n",
    "      X_Device_Country as `country_code`,    -- 필수/조건 열\n",
    "      log_create_time,                       -- 필수/조건 열\n",
    "      X_Device_SDK_VERSION as `dpv`,\n",
    "      mac_addr,               -- 필수 열  \n",
    "      x_device_sales_model as `sales_model_code`, -- 필수 열\n",
    "      context_name,                          -- 필수/조건 열  \n",
    "      message_id,                            -- 필수/조건 열\n",
    "      normal_log:job_type, -- {\"job_type\":\"Medication\",\"app_id\":\"voice\",\"update_type\":\"created\"}\n",
    "      normal_log:app_id,\n",
    "      normal_log:update_type\n",
    "  FROM kic_data_ods.tlamp.normal_log_webos25\n",
    "  WHERE 1=1\n",
    "      AND X_device_country = 'KR'\n",
    "      AND (context_name = 'reservance' AND message_id = 'NL_LIFEALIMI')\n",
    "      AND X_device_product = 'webOSTV 25'\n",
    "      AND (x_device_sales_model LIKE '65QNED85AKS%' OR x_device_sales_model LIKE '75QNED85AKS%') -- EASY TV 모델\n",
    "      AND date_ym >= '2025-10'\n",
    "      AND date_ym < '2025-12'  -- `25.10.01 ~ `25.11.09\n",
    "), fact_total_ud_by_job_and_app as (\n",
    "  select job_type, update_type, count(distinct mac_addr) as total_ud\n",
    "  from   raw_data \n",
    "  group by job_type, update_type \n",
    "), fact_count_ud_by_job_and_app as (\n",
    "  select job_type, app_id, update_type, count(mac_addr) as count_log, count(distinct mac_addr) as count_ud\n",
    "  from   raw_data\n",
    "  group by job_type, app_id, update_type\n",
    "  order by job_type, app_id, update_type\n",
    "), fact_total_ud_by_app as (\n",
    "  select count(distinct mac_addr) as total_ud\n",
    "  from   raw_data\n",
    "), fact_count_ud_by_app as (\n",
    "  select 'Total' as job_type, app_id, update_type, count(mac_addr) as count_log, count(distinct mac_addr) as count_ud\n",
    "  from   raw_data \n",
    "  group by app_id, update_type \n",
    ")\n",
    "\n",
    "select * \n",
    "from (\n",
    "  (select count.*, total.total_ud\n",
    "  from   fact_count_ud_by_job_and_app count\n",
    "  join   fact_total_ud_by_job_and_app total \n",
    "  on count.job_type = total.job_type and count.update_type = total.update_type\n",
    "  order by count.job_type, count.app_id)\n",
    "\n",
    "  union all \n",
    "\n",
    "  (select * from fact_count_ud_by_app, fact_total_ud_by_app)\n",
    ") t\n"
   ]
  },
