    "qr = f\"\"\"\n",
    "select \n",
    "    date_ym,\n",
    "    X_Device_Country as country_code,\n",
    "    'webOSTV 25' as platform_version,\n",
    "    DEVICE_NAME as soc,\n",
    "    normal_log:caller_id,\n",
    "    normal_log:app_id,\n",
    "    count(distinct mac_addr) as tv_counters,\n",
    "    count(mac_addr) as access_counters\n",
    "from kic_data_ods.tlamp.normal_log_webos25\n",
    "where 1=1\n",
    "and context_name = 'SAM'\n",
    "and message_id = 'NL_APP_LAUNCH_BEGIN'\n",
    "and date_ym = '2025-06'\n",
    "group by\n",
    "    date_ym,\n",
    "    X_Device_Country,\n",
    "    DEVICE_NAME,\n",
    "    normal_log:caller_id,\n",
    "    normal_log:app_id\n",
    "\"\"\"\n",
    "\n",
    "df = spark.sql(qr)"
   ]
