   "outputs": [],
   "source": [
    "%sql\n",
    "\n",
    "-- (1) ICS:NL_UVC_LIST  ∩  HOME:NL_HOME_SHOWN  의 월별(mac) 교집합 건수\n",
    "WITH ics AS (\n",
    "  SELECT DISTINCT date_ym, mac_addr\n",
    "  FROM kic_data_ods.tlamp.normal_log_webos24\n",
    "  WHERE date_ym IN ('2025-10', '2025-11', '2025-12')\n",
    "    AND X_Device_Country = 'IN'\n",
    "    AND X_Device_Product = 'webOSTV 24'\n",
    "    AND context_name = 'ICS'\n",
    "    AND message_id   = 'NL_UVC_LIST'\n",
    "),\n",
    "home AS (\n",
    "  SELECT date_ym, mac_addr\n",
    "  FROM kic_data_ods.tlamp.normal_log_webos24\n",
    "  WHERE date_ym IN ('2025-10', '2025-11', '2025-12')\n",
    "    AND X_Device_Country = 'IN'\n",
    "    AND X_Device_Product = 'webOSTV 24'\n",
    "    AND context_name = 'com.webos.app.home'\n",
    "    AND message_id   = 'NL_HOME_SHOWN'\n",
    ")\n",
    "SELECT\n",
    "  h.date_ym,\n",
    "  COUNT(*) AS log_cnt,\n",
    "  COUNT(DISTINCT h.mac_addr) AS mac_cnt\n",
    "FROM home h\n",
    "JOIN ics i\n",
    "  ON h.date_ym = i.date_ym\n",
    " AND h.mac_addr = i.mac_addr\n",
    "GROUP BY h.date_ym\n",
    "ORDER BY h.date_ym;"
   ]
